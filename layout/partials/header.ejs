<header id="header">
  <a href="<%- url_for("/") %>">
    <div id="title">
      <h1><%= config.title %></h1>
    </div>
  </a>
  <div id="nav">
    <ul>
      <% for (var i in theme.menu) { %> 
        <li><a href="<%- url_for(theme.menu[i]) %>"><%= __('menu.'+i).replace('menu.', '') %></a></li>
      <% } %>
    </ul>
  </div>
</header>